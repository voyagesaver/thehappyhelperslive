          "_customHTML": "<section class=\"menu2 menu\" group=\"Menu\" plugins=\"DropDown, TouchSwipe\" global always-top position-absolute once=\"menu\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <input type=\"checkbox\" title=\"Collapsed\" name=\"collapsed\">\n        <input type=\"checkbox\" title=\"Transparent\" name=\"transparent\">\n        <input type=\"checkbox\" title=\"Sticky\" name=\"sticky\" checked>\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Icons\" name=\"showIcons\" checked>\n        <select title=\"Icons\" name=\"iconsAmount\" condition=\"showIcons\">\n            <option value=\"0\">0</option>\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\" selected>5</option>\n            <option value=\"6\">6</option>\n        </select>\n        <select title=\"Widget Count\" name=\"widgetCount\">\n            <option value=\"0\">0</option>\n            <option value=\"1\">1</option>\n            <option value=\"2\" selected>2</option>\n            <option value=\"3\">3</option>\n        </select>\n        <input type=\"checkbox\" title=\"Logo\" name=\"showLogo\">\n        <input type=\"checkbox\" title=\"Brand Name\" name=\"showBrand\" checked>\n        <input type=\"checkbox\" title=\"Button\" name=\"showButtons\" checked>\n        <select title=\"Menu Align\" name=\"menuAlign\">\n            <option value=\"left\">Left</option>\n            <option value=\"center\" selected>Center</option>\n            <option value=\"right\">Right</option>\n        </select>\n\n\n        <header>Size</header>\n        <input type=\"range\" title=\"Logo\" inline name=\"logoSize\" min=\"2\" max=\"8\" step=\"0.5\" value=\"4\" condition=\"showLogo\">\n        <header>Colors</header>\n        <input type=\"color\" title=\"Top Menu\" name=\"menutopColor\" value=\"#ffffff\">\n        <input type=\"color\" title=\"Menu\" name=\"menuBgColor\" value=\"#ffffff\">\n        <input type=\"color\" title=\"Dropdown\" name=\"drColor\" value=\"#ffffff\">\n        <input type=\"color\" title=\"Dropdown Border\" name=\"drbrColor\" value=\"#4f5b77\">\n        <input type=\"color\" title=\"Hover Items\" name=\"hoverItemsColor\" value=\"#00acee\">\n        <input type=\"color\" title=\"Hover Line\" name=\"hoverLineColor\" value=\"#00acee\">\n        <input type=\"color\" title=\"Icons Bg\" name=\"iconsBgColor\" value=\"#ffffff\" condition=\"showIcons\">\n        <input type=\"color\" title=\"Icons Bg Hover\" name=\"iconsHoverBgColor\" value=\"#eeeff2\" condition=\"showIcons\">\n        <input type=\"color\" title=\"Icons Hover\" name=\"iconsHoverolor\" value=\"#ffffff\" condition=\"showIcons\">\n        <input type=\"color\" title=\"Hamburger\" name=\"hamburgerColor\" value=\"#282828\">\n\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <nav class=\"navbar navbar-dropdown align-items-center\" mbr-class=\"{'navbar-fixed-top': sticky,\n        'navbar-toggleable-sm': !collapsed,\n        'collapsed': collapsed,\n        'bg-color transparent': transparent}\">\n        <div class=\"menu-content-top\" mbr-if=\"widgetCount > 0 &amp;&amp; iconsAmount > 0\">\n            <div class=\"icons-menu\" mbr-if=\"showIcons\">\n                <div class=\"soc-item\">\n                    <a href=\"https://www.facebook.com/HappyHelpersHTX/\" target=\"_blank\">\n                        <span mbr-icon class=\"mbr-iconfont socicon-facebook socicon\" style=\"color: rgb(99, 182, 143); fill: rgb(99, 182, 143);\"></span>\n                    </a>\n                </div>\n                <div class=\"soc-item\" mbr-if=\"iconsAmount > 1\">\n                    <a href=\"https://www.linkedin.com/in/the-happy-helpers-htx-6090b9203/\" target=\"_blank\">\n                        <span mbr-icon class=\"mbr-iconfont socicon-linkedin socicon\" style=\"color: rgb(99, 182, 143); fill: rgb(99, 182, 143);\"></span>\n                    </a>\n                </div>\n                <div class=\"soc-item\" mbr-if=\"iconsAmount > 2\">\n                    <a href=\"https://www.instagram.com/happy_helpers_htx/?hl=en\" target=\"_blank\">\n                        <span mbr-icon class=\"mbr-iconfont socicon-instagram socicon\" style=\"color: rgb(99, 182, 143); fill: rgb(99, 182, 143);\"></span>\n                    </a>\n                </div>\n                <div class=\"soc-item\" mbr-if=\"iconsAmount > 3\">\n                    <a href=\"https://twitter.com/happyhelpershtx\" target=\"_blank\">\n                        <span mbr-icon class=\"mbr-iconfont socicon-twitter socicon\" style=\"color: rgb(39, 158, 100); fill: rgb(39, 158, 100);\"></span>\n                    </a>\n                </div>\n                <div class=\"soc-item\" mbr-if=\"iconsAmount > 4\">\n                    <a href=\"https://mobiri.se\" target=\"_blank\">\n                        <span mbr-icon class=\"mbr-iconfont socicon-tiktok socicon\" style=\"color: rgb(99, 182, 143); fill: rgb(99, 182, 143);\"></span>\n                    </a>\n                </div>\n                <div class=\"soc-item\" mbr-if=\"iconsAmount > 5\">\n                    <a href=\"https://mobiri.se\" target=\"_blank\">\n                        <span mbr-icon class=\"mbr-iconfont socicon socicon-behance\"></span>\n                    </a>\n                </div>\n            </div>\n            <div class=\"right-wrap d-flex align-items-center\">\n                <div class=\"info-widget\">\n                    <a href=\"applynow.html\"><span mbr-icon class=\"widget-icon mbr-iconfont mobi-mbri-paper-plane mobi-mbri\" style=\"color: rgb(99, 182, 143); fill: rgb(99, 182, 143);\"></span></a>\n                    <div class=\"widget-content\" mbr-theme-style=\"display-4\">\n                        <p class=\"mbr-fonts-style mbr-regular widget-title\" data-app-selector=\".widget-title, .widget-icon\" mbr-theme-style=\"display-4\">Contact Us +17133809395</p>\n                        <p class=\"mbr-fonts-style mbr-regular widget-title\" data-app-selector=\".widget-title, .widget-icon\" mbr-theme-style=\"display-4\">info@thehappyhelpers.org<br></p>\n                    </div>\n                </div>\n                <div class=\"info-widget\" mbr-if=\"widgetCount > 1\">\n                    <a href=\"popup:#mbr-popup-3m\" data-toggle=\"modal\" data-target=\"#mbr-popup-3m\"><span mbr-icon class=\"widget-icon mbr-iconfont mobi-mbri-globe-2 mobi-mbri\" style=\"color: rgb(99, 182, 143); fill: rgb(99, 182, 143);\"></span></a>\n                    <div class=\"widget-content\" mbr-theme-style=\"display-4\">\n                        <p class=\"mbr-fonts-style mbr-regular widget-title\" data-app-selector=\".widget-title, .widget-icon\" mbr-theme-style=\"display-4\">Choose Language:</p>\n                        <p class=\"mbr-fonts-style mbr-regular widget-title\" data-app-selector=\".widget-title, .widget-icon\" mbr-theme-style=\"display-4\"><a href=\"bienvenueauthehappyhelpers.html\" class=\"text-black\">FRANCAIS</a>/<a href=\"index.html\" class=\"text-black text-primary\">ENGLISH</a></p>\n                    </div>\n                </div>\n                <div class=\"info-widget\" mbr-if=\"widgetCount > 2\">\n                    <span mbr-icon class=\"widget-icon mbr-iconfont mbri-pin\"></span>\n                    <div class=\"widget-content\" mbr-theme-style=\"display-4\">\n                        <p class=\"mbr-fonts-style mbr-regular widget-title\" data-app-selector=\".widget-title, .widget-icon\" mbr-theme-style=\"display-4\">Here we are</p>\n                        <p class=\"mbr-fonts-style mbr-regular widget-title\" data-app-selector=\".widget-title, .widget-icon\" mbr-theme-style=\"display-4\">NYC, USA</p>\n                    </div>\n                </div>\n            </div>\n        </div>\n        <div class=\"menu-bottom\">\n            <div class=\"menu-logo\" mbr-if=\"showLogo || showBrand\">\n                <div class=\"navbar-brand\">\n                    <span mbr-if=\"showLogo\" class=\"navbar-logo\">\n                        <a href=\"https://mobiri.se\">\n                            <img src=\"../_images/logo.png\" alt=\"Mobirise\" mbr-style=\"{'height': logoSize + 'rem'}\">\n                        </a>\n                    </span>\n                    <span mbr-if=\"showBrand\" mbr-theme-style=\"display-5\" mbr-buttons class=\"navbar-caption-wrap\" data-toolbar=\"-mbrBtnMove,-mbrBtnAdd,-mbrBtnRemove\"><a href=\"index.html\" class=\"brand-name mbr-bold text-info text-primary\" data-app-selector=\".brand-name\" data-app-placeholder=\"Type Text\">The Happy Helpers&nbsp;&nbsp;</a></span>\n                </div>\n            </div>\n            <div class=\"collapse navbar-collapse\" id=\"navbarSupportedContent\">\n                <ul mbr-menu class=\"navbar-nav nav-dropdown js-float-line\" mbr-theme-style=\"display-4\" mbr-class=\"{'nav-right': !showButtons,'navbar-nav-top-padding': isPublish&&!showBrand&&!showLogo}\"><li class=\"nav-item\">\n                        <a class=\"nav-link link text-info text-primary\" href=\"index.html\" data-app-selector=\".nav-link,.dropdown-item\" data-app-placeholder=\"Type Text\">Home</a>\n                    </li>\n                    \n                    \n                    <li class=\"nav-item dropdown\">\n                        <a class=\"nav-link link dropdown-toggle text-info\" href=\"#\" data-app-selector=\".nav-link,.dropdown-item\" data-app-placeholder=\"Type Text\" data-toggle=\"dropdown-submenu\" aria-expanded=\"false\">\n                            About Us</a><div class=\"dropdown-menu\"><a class=\"dropdown-item text-info text-primary\" href=\"index.html#header1-1z\" data-app-selector=\".nav-link,.dropdown-item\" data-app-placeholder=\"Type Text\">Who We Are</a><a class=\"dropdown-item text-info text-primary\" href=\"index.html#content5-8\" data-app-selector=\".nav-link,.dropdown-item\" data-app-placeholder=\"Type Text\">Our Mission</a><a class=\"dropdown-item text-info text-primary\" href=\"index.html#content5-16\" data-app-selector=\".nav-link,.dropdown-item\" data-app-placeholder=\"Type Text\">Our Values</a><a class=\"dropdown-item text-info text-primary\" href=\"ourteam.html\" data-app-selector=\".nav-link,.dropdown-item\" data-app-placeholder=\"Type Text\">Our Team</a></div>\n                    </li><li class=\"nav-item dropdown\"><a class=\"nav-link link dropdown-toggle text-info\" href=\"#\" data-app-selector=\".nav-link,.dropdown-item\" data-app-placeholder=\"Type Text\" aria-expanded=\"false\" data-toggle=\"dropdown-submenu\">Our Services</a><div class=\"dropdown-menu\"><a class=\"dropdown-item text-info text-primary\" href=\"index.html#content9-3f\" data-app-selector=\".nav-link,.dropdown-item\" data-app-placeholder=\"Type Text\" aria-expanded=\"false\">Benefits</a></div></li><li class=\"nav-item\"><a class=\"nav-link link text-info text-primary\" href=\"getinvolved.html\" data-app-selector=\".nav-link,.dropdown-item\" data-app-placeholder=\"Type Text\">Get Involved</a></li></ul>\n            </div>\n            <div class=\"menu-content-right\">\n                <div mbr-if=\"showButtons\" mbr-buttons mbr-theme-style=\"display-4\" class=\"navbar-buttons mbr-section-btn\"><a class=\"btn btn-sm btn-secondary\" href=\"applynow.html\" data-app-placeholder=\"Type Text\">Apply Now</a> <a class=\"btn btn-sm btn-primary\" href=\"https://www.paypal.com/cgi-bin/webscr?cmd=_donations&item-name=Donate%20to%20The%20Happy%20Helpers&item-number=&currency_code=USD\" data-app-placeholder=\"Type Text\" target=\"_blank\">Donate&nbsp;</a></div>\n            </div>\n            <button class=\"navbar-toggler \" type=\"button\" data-toggle=\"collapse\" data-target=\"#navbarSupportedContent\" aria-controls=\"navbarSupportedContent\" aria-expanded=\"false\" aria-label=\"Toggle